<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane prefHeight="640.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.wat.jfk.controllers.PrimaryViewController">
   <children>
      <HBox prefHeight="538.0" prefWidth="1100.0">
         <children>
            <Pane prefHeight="490.0" prefWidth="227.0">
               <children>
                  <ListView fx:id="classList" layoutX="9.0" layoutY="14.0" prefHeight="513.0" prefWidth="212.0" />
               </children>
            </Pane>
            <Pane prefHeight="490.0" prefWidth="238.0">
               <children>
                  <ListView fx:id="fieldList" layoutX="2.0" layoutY="56.0" prefHeight="470.0" prefWidth="237.0" />
                  <ComboBox fx:id="choiceBar" layoutX="4.0" layoutY="14.0" onAction="#chooseField" prefHeight="31.0" prefWidth="226.0" />
               </children></Pane>
            <Pane prefHeight="535.0" prefWidth="422.0">
               <children>
                  <TextArea fx:id="textArea" layoutX="10.0" layoutY="13.0" prefHeight="513.0" prefWidth="403.0" />
               </children></Pane>
            <Pane prefHeight="538.0" prefWidth="213.0">
               <children>
                  <Label fx:id="actionLabel" layoutX="6.0" layoutY="14.0" prefHeight="21.0" prefWidth="198.0" text="Label" />
                  <Button fx:id="addButton" alignment="CENTER" layoutX="20.0" layoutY="50.0" minWidth="40.0" mnemonicParsing="false" onAction="#addAction" prefWidth="150.0" text="Add" />
                  <Button fx:id="removeButton" alignment="CENTER" layoutX="20.0" layoutY="95.0" minWidth="40.0" mnemonicParsing="false" onAction="#removeAction" prefWidth="150.0" text="Remove" />
                  <Button fx:id="overwriteButton" alignment="CENTER" layoutX="20.0" layoutY="140.0" minWidth="40.0" mnemonicParsing="false" onAction="#overwriteAction" prefWidth="150.0" text="Overwrite" />
                  <Button fx:id="addBeginningButton" layoutX="20.0" layoutY="185.0" minWidth="40.0" mnemonicParsing="false" onAction="#addAtBeginningMethod" prefWidth="150.0" text="Add at beginning" />
                  <Button fx:id="addEndButton" layoutX="20.0" layoutY="230.0" minWidth="40.0" mnemonicParsing="false" onAction="#addAtEndMethod" prefWidth="150.0" text="Add at end" />
                  <Button fx:id="addClassButton" layoutX="20.0" layoutY="362.0" minWidth="40.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="150.0" text="Add" onAction="#addClassAction" />
                  <Button fx:id="removeClassButton" layoutX="20.0" layoutY="406.0" minWidth="40.0" mnemonicParsing="false" prefWidth="150.0" text="Remove" onAction="#removeClassAction" />
                  <ComboBox fx:id="classChoice" layoutX="20.0" layoutY="319.0" prefWidth="150.0" />
               </children></Pane>
         </children>
      </HBox>
      <HBox layoutY="533.0" prefHeight="107.0" prefWidth="1100.0">
         <children>
            <Pane prefHeight="107.0" prefWidth="683.0">
               <children>
                  <Button fx:id="loadJarButton" layoutX="462.0" layoutY="38.0" mnemonicParsing="false" onAction="#loadJar" text="load .jar file" />
                  <TextField fx:id="jarPath" layoutX="14.0" layoutY="38.0" prefHeight="31.0" prefWidth="438.0" />
                  <ProgressIndicator fx:id="progressIndicator" layoutX="571.0" layoutY="43.0" progress="0.0" />
                  <Circle fx:id="indicatorCircle" fill="#feff1f" layoutX="639.0" layoutY="56.0" radius="16.0" stroke="BLACK" strokeType="INSIDE" />
               </children>
            </Pane>
            <Pane prefHeight="107.0" prefWidth="212.0">
               <children>
                  <Button fx:id="exportJarButton" layoutX="42.0" layoutY="38.0" mnemonicParsing="false" onAction="#exportJar" text="export to .jar" />
               </children>
            </Pane>
         </children>
      </HBox>
   </children>
</AnchorPane>
